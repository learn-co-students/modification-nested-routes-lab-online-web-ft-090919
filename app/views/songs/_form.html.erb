<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br>

  <%= f.label :artist, "Existing Artist" %>
  <%= f.collection_select :artist_id, Artist.all, :id, :name %><br>

  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <br />

  <%= f.submit %>
<% end %>
